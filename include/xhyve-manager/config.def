// CFG(section, name, default)

CFG(machine, type, "linux")

CFG(boot, options, "earlyprintk=serial console=ttyS0 acpi=off")
CFG(memory, size, "1G")
CFG(processor, cpus, "1")
CFG(storage_devices, storage, "virtio-blk")
CFG(storage_devices, internal, "storage/internal.hdd")
CFG(storage_devices, external, "")
CFG(networking, network, "virtio-net")
CFG(pci, bridge_dev, "hostbridge")
CFG(pci, lpc_dev, "lpc")
CFG(lpc, link, "stdio")

#undef CFG

